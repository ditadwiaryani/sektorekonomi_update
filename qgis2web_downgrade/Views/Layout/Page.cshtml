    <meta name="author" content="">


    <title>@ViewData["Title"] - Data Sektor Ekonomi Mergangsan</title>

    <!-- Custom fonts for this template-->
    <link href="~/Assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/Assets/css/sb-admin-2.css" rel="stylesheet">
    @*<link href="css/sb-admin-2.min.css" rel="stylesheet">*@

</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Nav Item - Dashboard -->
            <li id="lihome" class="nav-item">
                <a class="nav-link" href="./">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Home</span>
                </a>
            </li>
            <li id="lipemkot" class="nav-item">
                <a class="nav-link" href="./linkpemkot">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Kecamatan Mergangsan</span>
                </a>
            </li>
            <li id="liform" class="nav-item">
                <a class="nav-link" href="./linkform">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Form Data</span>
                </a>
            </li>
            <!-- Nav Item - Charts -->
            <li id="liwebmap" class="nav-item">
                <a class="nav-link" href="./webmap">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Webmap</span>
                </a>
            </li>

            <!-- Nav Item - Tables -->
            <li id="linews" class="nav-item">
                <a class="nav-link" href="./news">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Laporan</span>
                </a>
            </li>

            <!-- Nav Item - Pages Collapse Menu -->

        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow" style="height:3rem;margin-bottom:0 !important">

                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>

                    
                </nav>
                @*<div id="content">*@
                    @RenderBody()
                @*</div>*@
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                @*<div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->

        </div>*@
                @*<div class="container-fluid">



        </div>*@
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            @*<footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Your Website 2020</span>
                    </div>
                </div>
            </footer>*@
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="login.html">Logout</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="~/Assets/vendor/jquery/jquery.min.js"></script>
    <script src="~/Assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Assets/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/Assets/js/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="~/Assets/vendor/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Assets/js/demo/chart-area-demo.js"></script>
    <script src="~/Assets/js/demo/chart-pie-demo.js"></script>
    <script>
        var page = '@ViewBag.Title';
        $('#lihome').removeClass("active");
        $('#liwebmap').removeClass("active");
        $('#linews').removeClass("active");
        $('#liform').removeClass("active");
        $('#lipemkot').removeClass("active");

        if (page == "Home") {
            $('#lihome').addClass("active");
        } else if (page.includes("Web")) {
            $('#liwebmap').addClass("active");
        } else if (page.includes("News")) {
            $('#linews').addClass("active");
        } else if (page.includes("Form")) {
            $('#liform').addClass("active");
        } else if (page.includes("Kecamatan")) {
            $('#lipemkot').addClass("active");
        }

    </script>
    @RenderSection("Scripts", required: false)

</body>

</html>
